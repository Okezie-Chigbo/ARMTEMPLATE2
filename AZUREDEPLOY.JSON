{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {"name of storage acct": {
        "type": "string",
        "metadata": {
            "description": "Enter name of storage acct"
        }
    }},
    "functions": [],
    "variables": {},
    "resources": [{
        "name": "[parameters('name of storage acct')]",
        "type": "Microsoft.Storage/storageAccounts",
        "apiVersion": "2023-01-01",
        "tags": {
            "displayName": "Name of storage acct"
        },
        "location": "[resourceGroup().location]",
        "kind": "StorageV2",
        "sku": {
            "name": "Premium_LRS",
            "tier": "Premium"
        }
    }]
    "outputs": {}
}